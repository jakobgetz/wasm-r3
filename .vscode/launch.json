    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "jsc-replay",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/jsc/bin_0/replay.js",
            "args": [
                "run"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "test node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/tests/run-tests.cjs",
            "args": [
                "node",
                "-t",
                "mem-exp-host-mod-pingpong",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "test offline",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/tests/run-tests.cjs",
            "args": [
                "offline",
                "-t",
                "frame-in-frame"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "test online",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/tests/run-tests.cjs",
            "args": [
                "online",
                "-t",
                "game-of-life",
                "-c"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "sqllite",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/src/cli/main.cjs",
            "args": [
                "https://mbbill.github.io/JSC.js/demo/index.html",
                "-b",
                "jsc2",
                "-e"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "gameoflife",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/src/cli/main.cjs",
            "args": [
                "https://playgameoflife.com"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dbg-generator",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/src/cli/dbg-gen.cjs",
            "args": []
        },
        {
            "type": "node",
            "request": "launch",
            "name": "dbg-replay-trace",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/src/cli/dbg-replay.cjs",
            "args": []
        },
    ]
}